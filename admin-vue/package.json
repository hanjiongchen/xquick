{
  "name": "xquick-admin-vue",
  "version": "1.0.0",
  "description": "xquick admin desktop based on elementui",
  "author": "Charles <zhangchaoxu@gmail.com>",
  "license": "Apache License",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:test": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build --mode production",
    "build:uat": "vue-cli-service build --mode production.uat",
    "lint": "vue-cli-service lint",
    "et": "node_modules/.bin/et",
    "et:init": "node_modules/.bin/et -i",
    "et:list": "gulp themes"
  },
  "dependencies": {
    "ali-oss": "^6.6.0",
    "axios": "^0.19.2",
    "babel-plugin-component": "^1.1.1",
    "crypto-js": "^4.0.0",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.13.1",
    "element-ui": "^2.13.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-load-plugins": "^1.6.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.15",
    "dayjs": "^1.8.24",
    "node-sass": "^4.12.0",
    "qs": "^6.9.3",
    "quill": "^1.3.7",
    "sass-loader": "^7.1.0",
    "screenfull": "^3.3.3",
    "svg-sprite-loader": "^4.1.6",
    "vue": "^2.6.10",
    "vue-i18n": "^8.16.0",
    "vue-json-viewer": "^2.2.11",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/eslint-config-standard": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "babel-eslint": "8.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "engines": {
    "node": ">= 8.11.1",
    "npm": ">= 5.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "element-theme": {
    "config": "./src/element-ui/theme-variables.scss",
    "out": "./src/element-ui/theme",
    "minimize": true,
    "browsers": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  }
}
